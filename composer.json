{
  "name": "mceikens/symfony-kubernetes-healthz-check",
  "description": "Symfony healthz check bundle for kubernetes",
  "homepage": "https://www.mceikens.de",
  "type": "symfony-bundle",
  "keywords": [
    "MCEikens",
    "mceikens",
    "Symfony",
    "kubernetes",
    "k8s",
    "monitoring",
    "healthcheck",
    "doctrine",
    "Symfony-Health-Check",
    "Symfony-Health-Check-Bundle",
    "Symfony-Health-Check-Doctrine"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Eike Drost",
      "email": "eike.drost@mceikens.de",
      "role": "Lead Software Engineer"
    }
  ],
  "require": {
    "php": ">=8.0.2",
    "symfony/framework-bundle": "6.0.*"
  },
  "require-dev": {
    "ext-json": "*",
    "symfony/yaml": "6.0.*"
  },
  "autoload": {
    "psr-4": {
      "SymfonyKubernetesHealthzCheck\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SymfonyKubernetesHealthzCheck\\Tests\\": "tests"
    }
  },
  "scripts": {
    "composer-validate": [
      "composer validate"
    ]
  }
}
