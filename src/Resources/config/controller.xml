<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
    http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults autowire="true" autoconfigure="true" public="true" />

        <service public="true" id="MCEikens\SymfonyKubernetesHealthzCheck\Controller\Healthz\LivenessProbeController" class="MCEikens\SymfonyKubernetesHealthzCheck\Controller\Healthz\LivenessProbeController">
            <tag name="container.service_subscriber" />
        </service>
        <service public="true" id="MCEikens\SymfonyKubernetesHealthzCheck\Controller\Healthz\ReadinessProbeController" class="MCEikens\SymfonyKubernetesHealthzCheck\Controller\Healthz\ReadinessProbeController">
            <tag name="container.service_subscriber" />
        </service>
    </services>
</container>